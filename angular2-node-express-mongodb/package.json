{
  "name": "mws-ng2",
  "version": "2.0.0-beta",
  "description": "MWS",
  "repository": {
    "type": "svn",
    "url": ""
  },
  "scripts": {
    "start": "npm run _clean-dev && tsc && concurrently \"tsc -w\" \"node src/server/app.js\" ",
    "test": "tsc && npm run _run-tests",
    "build": "npm run _clean-dist && tsc --outDir dist && npm run _inline-templates && npm run _update-server && npm run _bundle && rm -rf dist/client && npm run _inject-bundle",
    "serve-dev": "npm start",
    "serve-dist": "node dist/server/app.js",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings",
    "install": "node ./node_modules/npm-svn/src/scanner.js",
    "postinstall": "typings install",
    "_run-tests": "karma start karma.conf.js",
    "_bundle": "browserify -s main dist/client/app/main.js > dist/bundle.js && npm run _minify",
    "_minify": "uglifyjs dist/bundle.js --screw-ie8 --compress --mangle --output dist/bundle.min.js",
    "_inline-templates": "gulp inline-templates",
    "_inject-bundle": "gulp inject-bundle",
    "_clean-dev": "find src/client/app src/server -name '*.js*' -exec sh -c 'rm \"$0\"' {} \\;",
    "_clean-dist": "rm -rf dist",
    "_update-server": "sed -i '' 's/src\\/client/dist/' dist/server/app.js"
  },
  "dependencies": {
    "@angular/common": "2.1.1",
    "@angular/compiler": "2.1.1",
    "@angular/core": "2.1.1",
    "@angular/forms": "2.1.1",
    "@angular/http": "2.1.1",
    "@angular/platform-browser": "2.1.1",
    "@angular/platform-browser-dynamic": "2.1.1",
    "@angular/router": "3.1.1",
    "body-parser": "^1.15.2",
    "bootstrap": "^3.3.6",
    "core-js": "^2.4.1",
    "crypto-js": "^3.1.6",
    "express": "^4.14.0",
    "jsonwebtoken": "^7.1.9",
    "mongodb": "^2.2.11",
    "node-uuid": "^1.4.7",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.12",
    "systemjs": "0.19.27",
    "zone.js": "^0.6.23"
  },
  "devDependencies": {
    "browserify": "^13.1.0",
    "concurrently": "^2.2.0",
    "gulp": "^3.9.1",
    "gulp-inject": "^4.1.0",
    "gulp-inline-ng2-template": "^3.0.1",
    "jasmine-core": "^2.5.2",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-htmlfile-reporter": "^0.3.4",
    "karma-jasmine": "^1.0.2",
    "npm-svn": "^0.2.0",
    "typescript": "^2.0.2",
    "typings": "^1.3.2",
    "uglify-js": "^2.7.3"
  },
  "svnDependencies": {
    "snap": "svn://usmlrs100.arrow.com/ecs_code_repositary/svn/repositories/static_files/portal/snap"
  }
}
